<tryton>
    <data group="1">
      <record model="ir.message" id="invalid_sale_state">
          <field name="text">You cannot modify the quantity of Sale "%(sale)s" because it is not in state "Confirmed" or "Progressing", or it has any Invoice or Shipment exception or it is already Paid or Sent.</field>
      </record>
      <record model="ir.message" id="quantity_already_delivered">
          <field name="text">Quantity already delivered!'</field>
      </record>
      <record model="ir.message" id="no_updateable_move">
          <field name="text">There is no updateable move available!</field>
      </record>
      <record model="ir.message" id="quantity_already_invoiced">
          <field name="text">Quantity already invoiced!</field>
      </record>
      <record model="ir.message" id="no_updateable_line">
          <field name="text">There is no updateable invoice line available!</field>
      </record>
    </data>
</tryton>
